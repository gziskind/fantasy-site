<!DOCTYPE html>
<html lang="en">
  <head>
    <%= erb :head %>

    <script src="/js/adminLogController.js"></script>
  </head>

  <body ng-app="aepi-fantasy" ng-controller="AdminLogController" ng-cloak>

    <div class="container">
      <!-- Static navbar -->
      <%= erb :header %>
      
      <div class="container-fluid">
        <div class="row">
          <div class="col-sm-12 col-md-12 terminal">
            <div ng-repeat="message in logMessages | orderBy:'time'">{{message.time | date:"yyyy/MM/dd HH:mm:ss"}} - [{{message.level}}] {{message.message}}</div>
          </div>
        </div>
      </div>



    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="/lib/bootstrap.min.js"></script>
  </body>
</html>
